source /etc/network/interface.d/*

#====[Rede Externa - WAN (enp0s3)]====
auto enp0s3
iface enp0s3 inet static
address 192.168.0.102
netmask 24
#====[Rede Interna - LAN IPv4 (enp0s8)]====
auto enp0s8
iface 172.16.1.1
netmask 24
#====[Rede Interna - LAN IPv4 (enp0s9)]====
auto enp0s9
iface enp0s9 inet static
address 10.10.10.1
netmask 24


